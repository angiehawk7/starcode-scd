<?xml version="1.0" encoding="UTF-8"?>
<ci ns="opencode">
    <!--对象类型-->
    <id>Project</id>
    <!--对象类型名称-->
    <name>${business.opencode.Project.name}</name>
    <!--继承路径-->
    <path>root</path>
    <!--描述-->
    <description></description>
    <!--显示图标-->
    <icon></icon>
    <!--指令集合-->
    <actions>
        <action>
            <!--指令ID-->
            <id></id>
            <!--指令名称-->
            <name></name>
            <!--指令作用域：Static,Instance-->
            <scope></scope>
            <!--显示条件-->
            <ctl_condition></ctl_condition>
            <!--指令执行模板-->
            <run_temlate></run_temlate>
        </action>
    </actions>
    <!--对象属性-->
    <attrs>
        <attr>
            <!--属性ID-->
            <id></id>
            <!--属性名称-->
            <name></name>
            <!--数据类型-->
            <d_type></d_type>
            <!--是否主键-->
            <is_primary></is_primary>
            <!--是否可编辑-->
            <is_editor></is_editor>
            <!--提示信息-->
            <tip></tip>
            <!--校验表达式-->
            <validate></validate>
        </attr>
    </attrs>
</ci>